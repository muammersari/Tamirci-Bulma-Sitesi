{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/jquery/"
    },
    {
      "library": "twitter-bootstrap@4.6.0",
      "destination": "wwwroot/lib/twitter-bootstrap/"
    },
    {
      "library": "bootbox.js@5.5.2",
      "destination": "wwwroot/lib/bootbox.js/"
    },
    {
      "library": "bootstrap-icons@1.4.0",
      "destination": "wwwroot/bootstrap-icons/"
    },
    {
      "library": "signalr.js@2.4.1",
      "destination": "wwwroot/signalr.js/"
    },
    {
      "library": "aspnet-signalr@1.0.27",
      "destination": "wwwroot/aspnet-signalr/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "microsoft-signalr@3.1.14",
      "destination": "wwwroot/signalR/microsoft-signalr/",
      "files": [
        "signalr.min.js"
      ]
    }
  ]
}